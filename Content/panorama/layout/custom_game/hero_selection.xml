		<Panel id="HeroSelection_LifesSelector" class="HeroSelection_LifesSelector" hittest="false">
			<Label text="Lifes"/>
			<Panel class="PlusMinus">
				<Button class="Minus" onactivate="ValueChange('nStartingLifes', -1);">
                    <Label id="Minus" text="-"/>
                </Button>
